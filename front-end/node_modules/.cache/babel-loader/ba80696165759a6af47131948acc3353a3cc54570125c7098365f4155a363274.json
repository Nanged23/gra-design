op: 50%;\n    transform: translateY(-50%);\n    background: none;\n    border: none;\n    color: #9ca3af;\n    cursor: pointer;\n  }\n  \n  .terms {\n    display: flex;\n    align-items: center;\n  }\n  \n  .terms input {\n    margin-right: 0.5rem;\n  }\n  \n  .terms label {\n    color: #9ca3af;\n  }\n  \n  .terms a {\n    color: white;\n    text-decoration: none;\n  }\n  \n  .terms a:hover {\n    text-decoration: underline;\n  }\n  \n  .submit-button {\n    width: 100%;\n    padding: 0.75rem;\n    border-radius: 0.5rem;\n    background-color: #8b5cf6;\n    color: white;\n    font-weight: 500;\n    border: none;\n    cursor: pointer;\n  }\n  \n  .submit-button:hover {\n    background-color: #7c3aed;\n  }\n  \n  .social-login {\n    margin-top: 2rem;\n  }\n  \n  .divider {\n    display: flex;\n    align-items: center;\n    margin-bottom: 1rem;\n  }\n  \n  .divider hr {\n    flex-grow: 1;\n    border: none;\n    border-top: 1px solid #374151;\n  }\n  \n  .divider span {\n    padding: 0 1rem;\n    color: #9ca3af;\n  }\n  \n  .social-buttons {\n    display: flex;\n    gap: 1rem;\n  }\n  \n  .social-button {\n    flex: 1;\n    padding: 0.75rem;\n    border-radius: 0.5rem;\n    border: 1px solid #374151;\n    background-color: transparent;\n    color: white;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    gap: 0.5rem;\n    cursor: pointer;\n  }\n  \n  .social-button:hover {\n    background-color: #1f2937;\n  }\n  \n  .social-button img {\n    width: 1.25rem;\n    height: 1.25rem;\n  }\n  \n  @media (min-width: 1024px) {\n    .hero-section {\n      display: flex;\n    }\n  \n    .form-section {\n      width: 50%;\n      padding: 6rem;\n    }\n  }\n  </style>"],"mappings":"AAmHE,SAASA,GAAG,QAAQ,KAAI;AACxB,SAASC,OAAO,EAAEC,UAAU,QAAQ,iBAAgB;;;;;;;IAEpD,MAAMC,SAAS,GAAGH,GAAG,CAAC,EAAE;IACxB,MAAMI,QAAQ,GAAGJ,GAAG,CAAC,EAAE;IACvB,MAAMK,KAAK,GAAGL,GAAG,CAAC,EAAE;IACpB,MAAMM,QAAQ,GAAGN,GAAG,CAAC,EAAE;IACvB,MAAMO,YAAY,GAAGP,GAAG,CAAC,KAAK;IAC9B,MAAM