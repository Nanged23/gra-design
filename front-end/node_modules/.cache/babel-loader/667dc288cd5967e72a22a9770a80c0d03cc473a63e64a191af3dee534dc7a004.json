{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AuthContainer from './AuthContainer.vue';\nexport default {\n  components: {\n    AuthContainer\n  },\n  data() {\n    return {\n      email: '',\n      password: '',\n      code: '',\n      emailError: '',\n      passwordError: '',\n      codeError: '',\n      countdown: 0,\n      timer: null\n    };\n  },\n  methods: {\n    gotoLogin() {\n      this.$router.push('/login');\n    },\n    validateEmail() {\n      const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n      if (this.email && !emailRegex.test(this.email)) {\n        this.emailError = '邮箱不符合规范';\n      } else {\n        this.emailError = '';\n      }\n    },\n    validatePassword() {\n      const hasUpperCase = /[A-Z]/.test(this.password);\n      const hasLowerCase = /[a-z]/.test(this.password);\n      if (this.password && (!hasUpperCase || !hasLowerCase)) {\n        this.passwordError = '密码必须包含大小写字母';\n      } else {\n        this.passwordError = '';\n      }\n    },\n    validateCode() {\n      if (!this.code) {\n        this.codeError = '请输入验证码';\n      } else if (this.code.length !== 6) {\n        this.codeError = '验证码必须是6位数字';\n      } else {\n        this.codeError = '';\n      }\n    },\n    sendVerificationCode() {\n      if (this.countdown > 0) return;\n      this.validateEmail();\n      if (this.emailError) return;\n      this.countdown = 60;\n      this.timer = setInterval(() => {\n        if (this.countdown > 0) {\n          this.countdown--;\n        } else {\n          clearInterval(this.timer);\n        }\n      }, 1000);\n\n      // TODO: Implement actual verification code sending logic\n      console.log('Sending verification code to:', this.email);\n    },\n    handleSubmit() {\n      this.validateEmail();\n      this.validatePassword();\n      this.validateCode();\n      if (!this.emailError && !this.passwordError && !this.codeError) {\n        console.log('Registration attempted:', {\n          email: this.email,\n          code: this.code\n        });\n      }\n    }\n  },\n  beforeUnmount() {\n    if (this.timer) {\n      clearInterval(this.timer);\n    }\n  }\n};","map":{"version":3,"names":["AuthContainer","components","data","email","password","code","emailError","passwordError","codeError","countdown","timer","methods","gotoLogin","$router","push","validateEmail","emailRegex","test","validatePassword","hasUpperCase","hasLowerCase","validateCode","length","sendVerificationCode","setInterval","clearInterval","console","log","handleSubmit","beforeUnmount"],"sources":["/Users/dongliwei/Desktop/文档/CQUPT 课程/毕业设计/Front-end/front-end/src/components/init/register.vue"],"sourcesContent":["<template>\n  <auth-container :isRegister=\"true\">\n    <div class=\"register-form\">\n      <div class=\"logo\">\n        <img src=\"../../assets/logo.png\" alt=\"Mastery Hub Logo\" />\n      </div>\n\n      <form @submit.prevent=\"handleSubmit\">\n        <!-- Email Input -->\n        <div class=\"form-group\">\n          <div class=\"input-wrapper\">\n            <input type=\"email\" v-model=\"email\" placeholder=\"邮箱\" @blur=\"validateEmail\" :class=\"{ error: emailError }\">\n            <svg class=\"input-icon\" t=\"1734505635026\" viewBox=\"0 0 1024 1024\" version=\"1.1\"\n              xmlns=\"http://www.w3.org/2000/svg\" p-id=\"10395\" width=\"20\" height=\"20\">\n              <path\n                d=\"M805.56544 134.55872H218.43456c-75.74528 0-137.25184 61.50656-137.25184 137.25184v480.37888c0 75.74528 61.51168 137.25184 137.25184 137.25184h587.13088c75.74528 0 137.25696-61.50656 137.25696-137.25184V271.81056c-0.512-75.74528-61.51168-137.25184-137.25696-137.25184zM138.68544 695.28064V328.70912l183.53664 183.04512-183.53664 183.5264z m453.4016-154.94144c-21.1968 21.19168-49.77664 33.01888-80.3328 33.01888s-58.64448-11.83232-80.32768-33.01888L142.62784 251.04896c11.33568-36.96128 44.35456-61.11232 84.76672-61.11232h569.21088c40.41728 0 73.43616 23.6544 84.76672 61.11232l-289.28512 