n')\n    },\n    validateEmail() {\n      const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/\n      if (this.email && !emailRegex.test(this.email)) {\n        this.emailError = '邮箱不符合规范'\n      } else {\n        this.emailError = ''\n      }\n    },\n    validatePassword() {\n      const hasUpperCase = /[A-Z]/.test(this.password)\n      const hasLowerCase = /[a-z]/.test(this.password)\n\n      if (this.password && (!hasUpperCase || !hasLowerCase)) {\n        this.passwordError = '密码必须包含大小写字母'\n      } else {\n        this.passwordError = ''\n      }\n    },\n    validateCode() {\n      if (!this.code) {\n        this.codeError = '请输入验证码'\n      } else if (this.code.length !== 6) {\n        this.codeError = '验证码必须是6位数字'\n      } else {\n        this.codeError = ''\n      }\n    },\n    sendVerificationCode() {\n      if (this.countdown > 0) return\n      \n      this.validateEmail()\n      if (this.emailError) return\n\n      this.countdown = 60\n      this.timer = setInterval(() => {\n        if (this.countdown > 0) {\n          this.countdown--\n        } else {\n          clearInterval(this.timer)\n        }\n      }, 1000)\n\n      // TODO: Implement actual verification code sending logic\n      console.log('Sending verification code to:', this.email)\n    },\n    handleSubmit() {\n      this.validateEmail()\n      this.validatePassword()\n      this.validateCode()\n\n      if (!this.emailError && !this.passwordError && !this.codeError) {\n        console.log('Registration attempted:', {\n          email: this.email,\n          code: this.code\n        })\n      }\n    }\n  },\n  beforeUnmount() {\n    if (this.timer) {\n      clearInterval(this.timer)\n    }\n  }\n}\n</script>\n\n<style scoped>\n.register-form {\n  width: 100%;\n  max-width: 400px;\n}\n\n.logo {\n  text-align: center;\n  margin-bottom: 62px;\n}\n\n.logo img {\n  height: 100px;\n}\n\n.form-group {\n  margin-bottom: 20px;\n}\n\n.input-wrapper {\n  position: relative;\n  width: 100%;\n}\n\n.input-wrapper input {\n  width: 100%;\n  padding: 12px 12px 12px 40px;\n  border: 1px solid var(--border-color);\n  border-radius: 8px;\n  font-size: 16px;\n  transition: border-color 0.3s ease;\n}\n\n.input-wrapper .input-icon {\n  position: absolute;\n  left: 12px;\n  top: 50%;\n  transform: translateY(-50%);\n  pointer-events: none;\n}\n\n.input-wrapper input:focus {\n  outline: none;\n  border-color: var(--primary-color);\n}\n\n.input-wrapper input::placeholder {\n  color: #999;\n}\n\n.input-wrapper input.error {\n  border-color: #ff4d4f;\n}\n\n.verification-wrapper {\n  display: flex;\n  gap: 8px;\n}\n\n.verification-wrapper input {\n  padding-left: 12px;\n}\n\n.verification-btn {\n  padding: 0 16px;\n  background-color: var(--primary-color);\n  color: white;\n  border: none;\n  border-radius: 8px;\n  font-size: 14px;\n  cursor: pointer;\n  transition: background-color 0.2s;\n  white-space: nowrap;\n  min-width: 120px;\n}\n\n.verification-btn:disabled {\n  background-color: #ccc;\n  cursor: not-allowed;\n}\n\n.verification-btn:hover:not(:disabled) {\n  background-color: #238276;\n}\n\n.error-message {\n  color: #ff4d4f;\n  font-size: 14px;\n  margin-top: 4px;\n  display: block;\n}\n\n.sign-up-btn {\n  width: 100%;\n  padding: 12px;\n  background-color: var(--primary-color);\n  color: white;\n  border: none;\n  border-radius: 8px;\n  font-size: 16px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: background-color 0.2s;\n}\n\n.sign-up-btn:hover {\n  background-color: #238276;\n}\n\n.login-prompt {\n  text-align: center;\n  margin-top: 24px;\n  font-size: 14px;\n  color: #718096;\n}\n\n.login-prompt a {\n  color: var(--primary-color);\n  text-decoration: none;\n  font-weight: 500;\n}\n</style>"],"mappings":";AA8DA,OAAOA,aAAY,MAAO,qBAAoB;AAE9C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE,EAAE;MACjBC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE,CAAC;MACZC