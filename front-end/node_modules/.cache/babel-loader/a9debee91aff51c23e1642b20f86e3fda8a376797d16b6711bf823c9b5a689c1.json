{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.some.js\";\nexport default {\n  data() {\n    return {\n      archive: [{\n        name: '经济政治类',\n        bookIds: ['41139692', '22781912', '23601930']\n      }, {\n        name: '闲暇读',\n        bookIds: ['CB_Eqw9By9Fj5TN6fh6gw8Pp0jV', 'CB_76l5uR5rt5XU6bY6cj', '3300012747']\n      }],\n      books: [{\n        bookId: '23601930',\n        title: '富爸爸穷爸爸',\n        cover: 'https://cdn.weread.qq.com/weread/cover/68/YueWen_23601930/s_YueWen_23601930.jpg'\n      }, {\n        bookId: '22781912',\n        title: '动物农场',\n        cover: 'https://wfqqreader-1252317822.image.myqcloud.com/cover/912/22781912/s_22781912.jpg'\n      }]\n    };\n  },\n  computed: {\n    // 获取不在任何文件夹中的书籍\n    booksNotInFolders() {\n      return this.books.filter(book => !this.archive.some(folder => folder.bookIds.includes(book.bookId)));\n    }\n  },\n  methods: {\n    // 获取某个文件夹中包含的书籍\n    getBooksInFolder(bookIds) {\n      return this.books.filter(book => bookIds.includes(book.bookId));\n    }\n  }\n};","map":{"version":3,"names":["data","archive","name","bookIds","books","bookId","title","cover","computed","booksNotInFolders","filter","book","some","folder","includes","methods","getBooksInFolder"],"sources":["/Users/dongliwei/Desktop/文档/CQUPT 课程/毕业设计/Front-end/front-end/src/components/third-content/third_douban.vue"],"sourcesContent":["<template>\n    <div>\n      <!-- 遍历所有文件夹 -->\n      <div v-for=\"folder in archive\" :key=\"folder.name\">\n        <div class=\"folder\">\n          <h3>{{ folder.name }}</h3>\n          <!-- 渲染文件夹封面和书籍 -->\n          <div v-if=\"getBooksInFolder(folder.bookIds).length > 0\">\n            <img v-for=\"book in getBooksInFolder(folder.bookIds)\" :key=\"book.bookId\" :src=\"book.cover\" :alt=\"book.title\" />\n            <ul>\n              <li v-for=\"book in getBooksInFolder(folder.bookIds)\" :key=\"book.bookId\">{{ book.title }}</li>\n            </ul>\n          </div>\n        </div>\n      </div>\n  <p>zd</p>\n      <!-- 直接展示不在文件夹中的书 -->\n      <div v-for=\"book in booksNotInFolders\" :key=\"book.bookId\" class=\"book\">\n        <h4>{{ book.title }}</h4>\n        <img :src=\"book.cover\" :alt=\"book.title\" />\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    data() {\n      return {\n        archive: [\n          {\n            name: '经济政治类',\n            bookIds: ['41139692', '22781912', '23601930'],\n          },\n          {\n            name: '闲暇读',\n            bookIds: ['CB_Eqw9By9Fj5TN6fh6gw8Pp0jV', 'CB_76l5uR5rt5XU6bY6cj', '3300012747'],\n          },\n        ],\n        books: [\n          {\n            bookId: '23601930',\n            title: '富爸爸穷爸爸',\n            cover: 'https://cdn.weread.qq.com/weread/cover/68/YueWen_23601930/s_YueWen_23601930.jpg',\n          },\n          {\n            bookId: '22781912',\n            title: '动物农场',\n            cover: 'https://wfqqreader-1252317822.image.myqcloud.com/cover/912/22781912/s_22781912.jpg',\n          },\n          \n        ],\n      };\n    },\n    computed: {\n      // 获取不在任何文件夹中的书籍\n      booksNotInFolders() {\n        return this.books.filter(book => \n          !this.archive.some(folder => folder.bookIds.includes(book.bookId))\n        );\n      },\n    },\n    methods: {\n      // 获取某个文件夹中包含的书籍\n      getBooksInFolder(bookIds) {\n        return this.books.filter(book => bookIds.includes(book.bookId));\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  /* 根据需求自定义样式 */\n  .folder {\n    margin-bottom: 20px;\n  }\n  .book {\n    margin-bottom: 15px;\n  }\n  img {\n    width: 100px;\n    height: 150px;\n    margin-right: 10px;\n  }\n  ul {\n    list-style-type: none;\n    padding-left: 0;\n  }\n  </style>\n  "],"mappings":";;;AAyBE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CACP;QACEC,IAAI,EAAE,OAAO;QACbC,OAAO,EAAE,CAAC,UA