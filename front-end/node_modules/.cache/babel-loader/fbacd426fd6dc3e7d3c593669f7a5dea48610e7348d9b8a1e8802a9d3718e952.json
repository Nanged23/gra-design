u items */\n.submenu .value::before {\n  left: 0px;\n  height: 80%;\n  /* Make the indicator span the full height of the item */\n}\n\n/* 调整子菜单项的before伪元素 */\n\n.right {\n  padding: 5px;\n}\n\n.input {\n  top: 0;\n  left: 0;\n  position: fixed;\n  display: flex;\n  flex-direction: column;\n  width: 250px;\n  height: 100vh;\n  background-color: #0d1117;\n  justify-content: center;\n  border-radius: 10px;\n  transition: 1s;\n  padding: 10px;\n  overflow: hidden;\n}\n\n.value {\n  font-size: 15px;\n  background-color: transparent;\n  border: none;\n  padding: 10px;\n  color: white;\n  display: flex;\n  position: relative;\n  gap: 5px;\n  cursor: pointer;\n  border-radius: 10px;\n  transition: 1s;\n  box-sizing: border-box;\n}\n\n.value:not(:active):hover,\n.value:focus {\n  display: flex;\n  box-sizing: border-box;\n  border: 2px solid #1a1f24;\n  color: #637185;\n}\n\n.value:focus,\n.value:active {\n  background-color: #1a1f24;\n  outline: none;\n}\n\n.value::before {\n  content: \"\";\n  position: absolute;\n  top: 5px;\n  left: 0px;\n  width: 5px;\n  height: 80%;\n  background-color: #2f81f7;\n  border-radius: 5px;\n  opacity: 0;\n  transition: 1s;\n}\n\n.value:focus::before,\n.value:active::before {\n  opacity: 1;\n}\n\n.value svg {\n  width: 20px;\n}\n</style>\n<script>\nexport default {\n  computed: {\n    getCard() {\n      return this.hitokotoData\n    }\n  },\n  mounted() {\n    fetch('https://v1.hitokoto.cn', {\n    \n})\n      .then(function (res) {\n        console.log(res);\n        \n        return res.json();\n      })\n      .then(function (data) {\n        var hitokoto = document.getElementById('hitokoto');\n        console.log(data);\n\n        hitokoto.innerText = data.hitokoto;\n      })\n      .catch(function (err) {\n        console.error(err);\n      })\n  },\n  data() {\n    return {\n      hitokotoData: ''\n    };\n  },\n  methods: {\n    navigateTo(menu) {\n      this.$router.push(`/${menu}`);\n    }\n  }\n\n}\n</script> -->\n<style scoped>\n 