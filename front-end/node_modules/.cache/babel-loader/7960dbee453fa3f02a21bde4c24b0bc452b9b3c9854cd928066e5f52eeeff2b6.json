ref } from 'vue'\n  \n  const showSignup = ref(false)\n  </script>\n  \n  <style>\n  @import url('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css');\n  \n  * {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n  }\n  \n  .container {\n    min-height: 100vh;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    background: #f5f5f5;\n    padding: 20px;\n  }\n  \n  .auth-container {\n    position: relative;\n    width: 800px;\n    height: 500px;\n    background: white;\n    border-radius: 10px;\n    box-shadow: 0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22);\n    overflow: hidden;\n  }\n  \n  .card {\n    position: absolute;\n    top: 0;\n    height: 100%;\n    transition: all 0.6s ease-in-out;\n    padding: 50px;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    text-align: center;\n  }\n  \n  .login-card {\n    left: 0;\n    width: 50%;\n    background: #41B883;\n    color: white;\n    transform: translateX(0);\n  }\n  \n  .signup-card {\n    right: 0;\n    width: 50%;\n    background: white;\n    transform: translateX(0);\n  }\n  \n  .show-signup .login-card {\n    transform: translateX(100%);\n  }\n  \n  .show-signup .signup-card {\n    transform: translateX(-100%);\n  }\n  \n  .logo {\n    width: 80px;\n    margin-bottom: 20px;\n  }\n  \n  h2 {\n    margin-bottom: 20px;\n    font-size: 2em;\n  }\n  \n  p {\n    margin-bottom: 30px;\n    font-size: 14px;\n  }\n  \n  .switch-btn {\n    background: transparent;\n    border: 1px solid white;\n    color: white;\n    padding: 12px 45px;\n    border-radius: 25px;\n    font-weight: bold;\n    cursor: pointer;\n    transition: 0.3s;\n  }\n  \n  .switch-btn:hover {\n    background: white;\n    color: #41B883;\n  }\n  \n  .social-buttons {\n    display: flex;\n    gap: 10px;\n    margin-bottom: 20px;\n  }\n  \n  .social-btn {\n    width: 40px;\n    height: 40px;\n    border-radius: 50%;\n    border: 1px solid #ddd;\n    background: white;\n    cursor: pointer;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    transition: 0.3s;\n  }\n  \n  .social-btn:hover {\n    background: #f5f5f5;\n  }\n  \n  form {\n    display: flex;\n    flex-direction: column;\n    gap: 10px;\n    width: 100%;\n    max-width: 300px;\n  }\n  \n  input {\n    padding: 12px 15px;\n    border: 1px solid #ddd;\n    border-radius: 5px;\n    outline: none;\n  }\n  \n  .submit-btn {\n    background: #41B883;\n    color: white;\n    border: none;\n    padding: 12px 45px;\n    border-radius: 25px;\n    font-weight: bold;\n    cursor: pointer;\n    transition: 0.3s;\n  }\n  \n  .submit-btn:hover {\n    background: #3aa876;\n  }\n  \n  .mobile-switch {\n    display: none;\n    background: none;\n    border: none;\n    color: #666;\n    margin-top: 15px;\n    cursor: pointer;\n  }\n  \n  @media (max-width: 768px) {\n    .auth-container {\n      width: 100%;\n      max-width: 400px;\n      height: auto;\n    }\n  \n    .card {\n      position: relative;\n      width: 100%;\n      padding: 30px;\n    }\n  \n    .login-card {\n      display: none;\n    }\n  \n    .signup-card {\n      transform: none;\n    }\n  \n    .show-signup .signup-card {\n      transform: none;\n    }\n  \n    .mobile-switch {\n      display: block;\n    }\n  }\n  </style>"],"mappings":"AAiCE,SAASA,GAAG,QAAQ,KAAI;;;;;;;IAExB,MAAMC,UAAU,GAAGD,GAAG,CAAC,KAAK","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}