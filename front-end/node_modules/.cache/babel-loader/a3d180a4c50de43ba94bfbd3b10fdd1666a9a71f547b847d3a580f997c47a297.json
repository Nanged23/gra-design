ack-btn {\n    padding: 0.5rem 1rem;\n    border-radius: 9999px;\n    background: rgba(255, 255, 255, 0.2);\n    color: white;\n    cursor: pointer;\n    transition: background-color 0.3s;\n  }\n  \n  .hero-section .back-btn:hover {\n    background-color: rgba(255, 255, 255, 0.3);\n  }\n  \n  .hero-section .hero-text {\n    position: absolute;\n    bottom: 20px;\n    left: 0;\n    width: 100%;\n    text-align: center;\n    color: white;\n  }\n  \n  .hero-section .title {\n    font-size: 2rem;\n    font-weight: bold;\n  }\n  \n  .hero-section .dots {\n    display: flex;\n    justify-content: center;\n    margin-top: 2rem;\n    gap: 0.5rem;\n  }\n  \n  .hero-section .dot {\n    width: 0.5rem;\n    height: 0.5rem;\n    background-color: rgba(255, 255, 255, 0.3);\n    border-radius: 50%;\n  }\n  \n  .hero-section .dot.active {\n    background-color: white;\n  }\n  \n  .hero-section .hero-img {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n    opacity: 0.5;\n  }\n  \n  .form-container {\n    width: 100%;\n    padding: 2rem;\n    background-color: #1f1f1f;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n  \n  .form-wrapper {\n    max-width: 400px;\n    width: 100%;\n    margin: 0 auto;\n  }\n  \n  .form-title {\n    font-size: 2rem;\n    font-weight: bold;\n    color: white;\n    margin-bottom: 1rem;\n  }\n  \n  .form-subtitle {\n    color: #bbb;\n    margin-bottom: 2rem;\n  }\n  \n  .form-subtitle .form-link {\n    color: white;\n    text-decoration: underline;\n  }\n  \n  .form {\n    display: flex;\n    flex-direction: column;\n    gap: 1.5rem;\n  }\n  \n  .form-group {\n    display: flex;\n    gap: 1rem;\n  }\n  \n  .form-input {\n    width: 100%;\n    padding: 0.75rem;\n    border-radius: 0.5rem;\n    background-color: #2a2a2a;\n    color: white;\n    border: 1px solid #333;\n    outline: none;\n  }\n  \n  .form-input:focus {\n    border-color: #9b4dff;\n  }\n  \n  .password-container {\n    position: relative;\n  }\n